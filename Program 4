//House Robber
//https://leetcode.com/problems/house-robber/description/
class Solution {
public:
    int rob(vector<int>& nums) {
         int n=nums.size();
        if(n==0) return 0;
        if(n==1) return nums[0];
        int home2=nums[0];
        int home1=max(nums[0],nums[1]);
        int current=home1;
        for(int i=2;i<n;i++){
            current=max(home1,home2+nums[i]);
            home2=home1;
            home1=current;
        }
        return current;
    }
};
